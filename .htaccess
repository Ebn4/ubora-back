<FilesMatch ".(inc|env|log|txt|json|xml|phar|lock|js)$">
Order Allow,Deny
Deny from all
</FilesMatch>

<Files .htaccess>
order allow,deny
deny from all
</Files>

#Interdiction de visualisation des repertoires du site:
Options All -Indexes


php_value upload_max_filesize 200M
php_value post_max_size 210M